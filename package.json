{
  "name": "rollupjs.org",
  "author": "Rich Harris",
  "version": "0.1.0",
  "devDependencies": {
    "clean-css": "^4.1.7",
    "console-group": "^0.3.3",
    "eslint": "^4.5.0",
    "eslint-plugin-html": "^3.2.0",
    "eslint-plugin-import": "^2.7.0",
    "highlight.js": "^9.12.0",
    "locate-character": "^2.0.1",
    "marked": "^0.3.6",
    "now": "^7.3.0",
    "rollup": "^0.48.0",
    "rollup-plugin-alias": "^1.3.1",
    "rollup-plugin-buble": "^0.15.0",
    "rollup-plugin-commonjs": "^8.1.0",
    "rollup-plugin-json": "^2.3.0",
    "rollup-plugin-node-resolve": "^3.0.0",
    "rollup-plugin-replace": "^1.1.1",
    "rollup-plugin-svelte": "^3.1.0",
    "rollup-plugin-uglify": "^2.0.1",
    "rollup-watch": "^4.3.1",
    "sander": "^0.6.0",
    "surge": "^0.19.0",
    "svelte": "^1.30.0"
  },
  "scripts": {
    "start": "node server/dist/bundle.js",
    "lint": "node node_modules/eslint/bin/eslint src --ext .js --ext .html",
    "update_overview": "node scripts/update_overview",
    "rollup": "rollup -c",
    "dev": "node scripts/prep && rollup -cw & npm start",
    "build": "node scripts/prep && npm run clean && rollup -c",
    "clean": "rm -rf client/dist && rm -rf server/dist && rm -rf service-worker/dist && mkdir -p client/dist && mkdir -p server/dist && mkdir -p service-worker/dist",
    "stage": "now",
    "deploy": "now && now alias",
    "deploy:ci": "now --token $NOW_TOKEN && now alias --token $NOW_TOKEN"
  },
  "dependencies": {
    "acorn": "^5.1.1",
    "codemirror": "^5.28.0",
    "compression": "^1.7.0",
    "express": "^4.15.4",
    "locate-character": "^2.0.1",
    "roadtrip": "^0.5.1"
  },
  "now": {
    "alias": "rollupjs.org",
    "files": [
      "examples",
      "guide",
      "scripts",
      "universal",
      "public",
      "server",
      "service-worker",
      "client",
      "rollup.config.js"
    ]
  }
}
