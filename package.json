{
  "name": "rollupjs.org",
  "author": "Rich Harris",
  "version": "0.1.0",
  "devDependencies": {
    "console-group": "^0.3.3",
    "css-loader": "^0.28.10",
    "eslint": "^4.18.1",
    "eslint-plugin-html": "^4.0.2",
    "eslint-plugin-import": "^2.9.0",
    "extract-text-webpack-plugin": "^3.0.2",
    "highlight.js": "^9.12.0",
    "marked": "^0.4.0",
    "mini-css-extract-plugin": "^0.4.0",
    "now": "^11.1.12",
    "sander": "^0.6.0",
    "style-loader": "^0.21.0",
    "svelte": "^2.6.3",
    "svelte-loader": "^2.3.3",
    "uglifyjs-webpack-plugin": "^1.1.8",
    "webpack": "^4.8.3"
  },
  "scripts": {
    "dev": "sapper dev",
    "build": "sapper build",
    "start": "node build",
    "lint": "node node_modules/eslint/bin/eslint src --ext .js --ext .html",
    "update_overview": "node scripts/update_overview",
    "clean": "rm -rf client/dist && rm -rf server/dist && rm -rf service-worker/dist && mkdir -p client/dist && mkdir -p server/dist && mkdir -p service-worker/dist",
    "stage": "now",
    "deploy": "now && now alias",
    "deploy:ci": "now --token $NOW_TOKEN && now alias --token $NOW_TOKEN"
  },
  "dependencies": {
    "codemirror": "^5.35.0",
    "compression": "^1.7.2",
    "cross-env": "^5.1.1",
    "express": "^4.16.3",
    "node-fetch": "^2.1.2",
    "sapper": "^0.12.0",
    "serve-static": "^1.13.2",
    "svelte-extras": "^2.0.2"
  },
  "now": {
    "alias": "rollupjs.org",
    "files": [
      "examples",
      "guide",
      "assets",
      "routes",
      "templates",
      "server.js",
      "webpack.*.config.js"
    ]
  }
}
